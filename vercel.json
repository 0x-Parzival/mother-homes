{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    },
    {
      "src": "motherhomes-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "src/app.ts"
    },
    {
      "source": "/api",
      "destination": "src/app.ts"
    },
    {
      "source": "/carousel_deployment/(.*)",
      "destination": "/motherhomes-frontend/carousel_deployment/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/motherhomes-frontend/assets/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/motherhomes-frontend/images/$1"
    },
    {
      "source": "/favicon.png",
      "destination": "/motherhomes-frontend/favicon.png"
    },
    {
      "source": "/(.*\\.[^/.]+)",
      "destination": "/motherhomes-frontend/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/motherhomes-frontend/index.html"
    }
  ]
}
