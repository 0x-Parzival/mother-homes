{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    },
    {
      "src": "motherhomes-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/app.ts"
    },
    {
      "src": "/api",
      "dest": "src/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/motherhomes-frontend/$1",
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/motherhomes-frontend/index.html"
    }
  ]
}
